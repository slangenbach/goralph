{
    "prd": "./.ralph/prd.json",
    "progress": "./.ralph/progress.txt",
    "prompt": "./.ralph/prompt.md",
    "model": "claude-opus-4.5",
    "tools": {
        "allow": [
            "shell(git:*)",
            "write"
        ],
        "deny": [
            "shell(git:push)"
        ]
    },
    "timeout": 60,
    "loglevel": "info"
}
